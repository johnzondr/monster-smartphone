<!-- Scaffolding generated by Casein v5.1.1.5 -->

<h2>Add a new home image (optimal size: 940 x 284)</h2>

<%= form_for @home_image, :url => casein_home_images_path do |f| %>
	
	<%= render :partial => 'casein/home_images/form', :locals => { :f => f } %>
	
	<p class="submits">
		<%= link_to 'Cancel', casein_home_images_path, :class => 'btn btn-mini btn-danger' %>
		<%= f.submit "Add new home image", :class => "btn btn-mini btn-primary" %>
	</p>

<% end %>

<%= content_for :sidebar do %>
	<li><%= link_to "#{casein_show_icon "th-list"}Back to list".html_safe, casein_home_images_path %></li>
<% end %>